package compititive;
//given an array count number of elements having at least one element greater than itself.

public class q1 {

    static int countEachElements(int [] arr){
        int count=0;
        int max=Integer.MIN_VALUE;
        for(int i=0;i<arr.length;i++){
            if(arr[i]>max){
                max=arr[i];
            }
        }
//return max;
for(int i=0;i<arr.length;i++){
    if(arr[i]!=max){
        count++;
    }
}
return count;
    }

  public static void main(String[] args) {
      int [] arr={4, 2, 5, -6, 1, 0, -4};
      System.out.println(countEachElements(arr));

    }
}
